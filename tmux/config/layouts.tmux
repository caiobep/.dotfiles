# htop
bind-key H \
	new-window -n htop htop \; \
	set-window-option allow-rename off \; \
	rename-window htop


# Gtop
bind-key G \
    new-window -n gtop gtop \; \
    set-window-option allow-rename off \; \
    rename-window gtop

# Cal
bind-key C \
	new-window -n cal cal \; \
	set-window-option allow-rename off \; \
	rename-window cal

